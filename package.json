{
  "name": "hello-electron-with-pouchdb",
  "version": "1.0.0",
  "description": "A minimal Electron application, with PouchDB",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-css": "node-sass --include-path scss scss/main.scss public/css/base.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-stopwatch-css": "node-sass --include-path stopwatch_scss stopwatch_scss/main.scss public/css/base2.css",
    "watch-stopwatch-css": "nodemon -e scss -x \"npm run build-stopwatch-css\""
  },
  "devDependencies": {
    "electron": "^1.4.12",
    "electron-rebuild": "^1.4.0",
    "eslint-config-google": "^0.9.1",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "angular": "^1.6.9",
    "angular-content-editable": "^1.2.3",
    "angular-route": "^1.6.9",
    "bootstrap": "^4.0.0",
    "eslint": "^4.19.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsdoc": "^3.5.5",
    "moment": "^2.22.0",
    "moment-duration-format": "^2.2.2",
    "popper.js": "^1.14.3",
    "pouchdb": "^6.1.0",
    "pouchdb-adapter-node-websql": "^6.1.0",
    "pouchdb-find": "^6.4.3",
    "pouchdb-upsert": "^2.2.0"
  }
}
